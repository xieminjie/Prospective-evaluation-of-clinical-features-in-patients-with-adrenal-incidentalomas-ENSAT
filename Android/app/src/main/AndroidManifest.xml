<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xieminjie.clientapp" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabbedDrawer"
            android:label="@string/title_activity_tabbed_drawer" >
        </activity>
        <activity
            android:name=".SurveyDetails"
            android:label="@string/title_activity_survey_details" >
        </activity>
        <activity
            android:name=".RecordDetail"
            android:label="@string/title_activity_record_detail" >
        </activity>
        <activity
            android:name=".OverallData"
            android:label="@string/title_activity_overall_data" >
        </activity>
        <activity
            android:name=".SingleComparison"
            android:label="@string/title_activity_single_comparison" >
        </activity>

    </application>

</manifest>
